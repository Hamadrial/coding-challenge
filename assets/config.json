{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "publicPath": "/app/public/wp-content/themes/coding-challenge-theme",
  "devUrl": "https://codingchallenge.local",
  "proxyUrl": "https://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "{components,views}/**/*.twig",
    "lib/**/*.php"
  ],
  "browsers": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ]
}
